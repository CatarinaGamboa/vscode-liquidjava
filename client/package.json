{
	"name": "liquid-java",
	"displayName": "LiquidJava",
	"description": "LiquidJava - Extending Java with Refinement Types",
	"version": "0.0.6",
	"publisher": "catarinagamboa",
	"engines": {
		"vscode": "^1.52.0"
	},
	"languages": [
		{
			"id": "java"
		}
	],
	"categories": [
		"Other"
	],
	"keywords": [
		"java",
		"test",
		"junit",
		"testng",
		"debug"
	],
	"activationEvents": [
		"onLanguage:java"
	],
	"main": "./out/extension.js",
	"contributes": {
		"configuration": {
			"type": "object",
			"title": "Liquid Java configuration",
			"properties": {
				"liquidJavaServer.maxNumberOfProblems": {
					"type": "number",
					"default": 100,
					"description": "Controls the maximum number of problems produced by the server."
				},
				"liquidJavaServer.trace.server": {
					"type": "string",
					"enum": [
						"off",
						"messages",
						"verbose"
					],
					"default": "off",
					"description": "Traces the communication between VSCode and the liquidJavaServer service."
				}
			}
		},
		"viewsContainers": {
			"activitybar": [
				
			]
		},
		"views": {
			
		},
		"commands": [
			
		],
		"menus": {

		}
	},
	"scripts": {
		"vscode:prepublish": "tsc",
		"compile": "tsc -watch -p ./",
		"watch": "tsc -watch -p ./",
		"lint": "eslint . --ext .ts,.tsx"
	},
	"extensionDependencies": [
		"redhat.java",
		"vscjava.vscode-java-debug"
	],
	"devDependencies": {
		"@types/mkdirp": "^0.5.2",
		"@types/node": "^12.12.0",
		"@types/rimraf": "^2.0.2",
		"@types/vscode": "^1.45.0",
		"@typescript-eslint/eslint-plugin": "^4.16.0",
		"@typescript-eslint/parser": "^4.16.0",
		"eslint": "^7.21.0",
		"typescript": "^4.2.2"
	},
	"dependencies": {
		"ftp": "^0.3.10",
		"get-port": "^5.1.1",
		"jsonc-parser": "^0.4.2",
		"minimist": "^0.2.1",
		"mkdirp": "^0.5.1",
		"rimraf": "^2.6.2",
		"vscode-languageclient": "^3.1.0"
	}
}
